{
  "name": "@project/db",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    },
    "./web/server": {
      "types": "./dist/web/server.d.ts",
      "import": "./dist/web/server.mjs",
      "require": "./dist/web/server.js"
    },
    "./web/client": {
      "types": "./dist/web/client.d.ts",
      "import": "./dist/web/client.mjs",
      "require": "./dist/web/client.js"
    },
    "./web/middleware": {
      "types": "./dist/web/middleware.d.ts",
      "import": "./dist/web/middleware.mjs",
      "require": "./dist/web/middleware.js"
    },
    "./mobile": {
      "types": "./dist/mobile/index.d.ts",
      "import": "./dist/mobile/index.mjs",
      "require": "./dist/mobile/index.js"
    }
  },
  "scripts": {
    "build": "tsup && tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.49.8"
  },
  "devDependencies": {
    "@project/tsconfig": "workspace:*",
    "@types/node": "^20.17.57",
    "tsup": "^8.0.0",
    "typescript": "^5"
  },
  "peerDependencies": {
    "expo-secure-store": ">=14.0.0",
    "next": ">=15.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "expo-secure-store": {
      "optional": true
    }
  }
}
